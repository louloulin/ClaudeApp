{
  "name": "claude-code-ui",
  "version": "1.5.0",
  "description": "A web-based UI for Claude Code CLI",
  "type": "module",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run server\" \"npm run client\"",
    "server": "node server/index.js",
    "client": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "start": "npm run build && npm run server",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "build:mobile": "npm run build && npx cap sync",
    "dev:android": "npx cap run android --livereload",
    "dev:ios": "npx cap run ios --livereload",
    "build:android": "npm run build && npx cap sync && npx cap build android",
    "build:ios": "npm run build && npx cap sync && npx cap build ios",
    "open:android": "npx cap open android",
    "open:ios": "npx cap open ios",
    "sync": "npx cap sync",
    "config:dev": "node scripts/configure-capacitor.js development",
    "config:test": "node scripts/configure-capacitor.js testing",
    "config:prod": "node scripts/configure-capacitor.js production",
    "release:patch": "node scripts/release.js patch",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major",
    "release:dry": "node scripts/release.js patch --dry-run",
    "deploy:dev": "node scripts/deploy.js development",
    "deploy:test": "node scripts/deploy.js testing",
    "deploy:prod": "node scripts/deploy.js production --force",
    "deploy:mobile": "node scripts/deploy.js mobile",
    "deploy:status": "node scripts/deploy.js status",
    "docker:build": "docker build -t claude-code-ui .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "verify:pwa": "node scripts/verify-pwa.js",
    "quick-start": "node scripts/quick-start.js",
    "quick-start:docker": "node scripts/quick-start.js docker",
    "quick-start:prod": "node scripts/quick-start.js production",
    "devcontainer:build": "node scripts/devcontainer-setup.js build",
    "devcontainer:start": "node scripts/devcontainer-setup.js start",
    "devcontainer:stop": "node scripts/devcontainer-setup.js stop",
    "devcontainer:rebuild": "node scripts/devcontainer-setup.js rebuild",
    "devcontainer:status": "node scripts/devcontainer-setup.js status",
    "devcontainer:cleanup": "node scripts/devcontainer-setup.js cleanup",
    "devcontainer:vscode": "node scripts/devcontainer-setup.js vscode"
  },
  "keywords": [
    "claude",
    "ai",
    "code",
    "ui",
    "assistant"
  ],
  "author": "Claude Code UI Contributors",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-code": "^1.0.24",
    "@capacitor/android": "^7.4.2",
    "@capacitor/cli": "^7.4.2",
    "@capacitor/core": "^7.4.2",
    "@capacitor/filesystem": "^7.1.2",
    "@capacitor/haptics": "^7.0.1",
    "@capacitor/ios": "^7.4.2",
    "@capacitor/network": "^7.0.1",
    "@capacitor/splash-screen": "^7.0.1",
    "@capacitor/status-bar": "^7.0.1",
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-markdown": "^6.3.3",
    "@codemirror/lang-python": "^6.2.1",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@tailwindcss/typography": "^0.5.16",
    "@uiw/react-codemirror": "^4.23.13",
    "@xterm/addon-clipboard": "^0.1.0",
    "@xterm/addon-webgl": "^0.18.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.2.0",
    "chokidar": "^4.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.515.0",
    "mime-types": "^3.0.1",
    "multer": "^2.0.1",
    "node-fetch": "^2.7.0",
    "node-pty": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.8.1",
    "tailwind-merge": "^3.3.1",
    "ws": "^8.14.2",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^2.1.9",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.32",
    "puppeteer": "^24.15.0",
    "sharp": "^0.34.2",
    "tailwindcss": "^3.4.0",
    "vite": "^7.0.4",
    "vitest": "^2.1.9"
  }
}
